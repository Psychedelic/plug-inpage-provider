  "jsnext:main": "dist/esm/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && npm-run-all --parallel build:** && node ./scripts/add-package.js",
